{
  "name": "langgraph-ai-sdk",
  "version": "1.0.20",
  "description": "Server utilities for LangGraph AI SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "development": "./src/testing/index.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "db:migrate": "tsx db/migrate.ts && tsx db/setup-checkpointer.ts"
  },
  "keywords": [
    "langgraph",
    "ai",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@langchain/core": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "ai": "^5.0.0",
    "langchain": "^1.0.0"
  },
  "devDependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@pollyjs/adapter-fetch": "^6.0.7",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@types/node": "^22.10.5",
    "ai": "^5.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "langchain": "^1.0.0",
    "pg": "^8.0.0",
    "tsdown": "^0.15.12",
    "tsx": "^4.20.6",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0",
    "xml-formatter": "^3.6.7"
  },
  "dependencies": {
    "@types/pg": "^8.15.6",
    "change-case": "^5.4.4",
    "drizzle-orm": "^0.44.7",
    "langgraph-ai-sdk-types": "workspace:*",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  }
}
