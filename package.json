{
  "name": "langgraph-ai-sdk-monorepo",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "typecheck": "tsc --noEmit",
    "langgraph:db:migrate": "pnpm --filter langgraph-ai-sdk db:migrate",
    "version:bump": "node scripts/version.mjs",
    "publish:all": "pnpm -r --filter './packages/*' publish --access public --no-git-checks",
    "release:patch": "node scripts/version.mjs patch && pnpm build && pnpm publish:all",
    "release:minor": "node scripts/version.mjs minor && pnpm build && pnpm publish:all",
    "release:major": "node scripts/version.mjs major && pnpm build && pnpm publish:all"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsdown": "^0.15.12",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}